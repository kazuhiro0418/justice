<tr>
  <td><%= board.id %></td>
  <td><%= board.title %></td>
  <td><%= board.name %></td>
  <td><%= board.created_at.to_s(:datetime) %></td>
  <td><%= board.updated_at.to_s(:datetime) %></td>
  <td><%= link_to '詳細', board_path(id: board.id) , class: "btn btn-outline-dark" %></td>
  <td><%= link_to '削除', board_path(id: board.id) , class: "btn btn-outline-dark", method: :delete %></td>
</tr>
